    <ApplicationPath>Y:\FullSets\Snes-moins-selection\Uniracers.7z</ApplicationPath>
    <Completed>false</Completed>
    <DateAdded>2016-01-18T23:04:04.4724831+01:00</DateAdded>
    <DateModified>2016-01-18T23:04:04.4724831+01:00</DateModified>
