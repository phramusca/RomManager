    <ApplicationPath>Y:\FullSets\Snes-moins-selection\Soukou Kihei Votoms - The Battling Road.7z</ApplicationPath>
    <Completed>false</Completed>
    <DateAdded>2016-01-18T23:09:36.2691332+01:00</DateAdded>
    <DateModified>2016-01-18T23:09:36.2691332+01:00</DateModified>
