    <ApplicationPath>Y:\FullSets\Snes-moins-selection\NFL Quarterback Club.7z</ApplicationPath>
    <Completed>false</Completed>
    <DateAdded>2016-01-18T23:03:05.9880935+01:00</DateAdded>
    <DateModified>2016-01-18T23:03:05.9880935+01:00</DateModified>
    <Developer>Iguana Entertainment</Developer>
    <Emulator>2672cea7-9a8a-4c8c-a53a-b0348d377ebb</Emulator>
