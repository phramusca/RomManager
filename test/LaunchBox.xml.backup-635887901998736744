    <ApplicationPath>Y:\FullSets\Snes-moins-selection\Lawnmower Man, The.7z</ApplicationPath>
    <Completed>false</Completed>
    <DateAdded>2016-01-18T23:17:09.7688702+01:00</DateAdded>
    <DateModified>2016-01-18T23:17:09.7688702+01:00</DateModified>
