    <ApplicationPath>Y:\FullSets\Megadrive\Pac-Man 2 - The New Adventures.7z</ApplicationPath>
    <Completed>false</Completed>
    <DateAdded>2016-01-18T23:28:26.6434521+01:00</DateAdded>
    <DateModified>2016-01-18T23:28:26.6434521+01:00</DateModified>
